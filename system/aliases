# Easier navigation: .., ..., ...., ....., ~ and -
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."
alias ~="cd ~" # `cd` is probably faster to type though
alias -- -="cd -"

# Shortcuts
alias d="cd Dropbox\ \(G*"
alias dp="cd Dropbox\ \(P*"
alias dl="cd ~/Downloads"
alias dt="cd ~/Desktop"
alias p="cd ~/projects"
alias h="history"
alias j="jobs"

# Get week number
alias week='date +%V'

# Git
alias ga="git add"
alias gl="git l"
alias gs="gws"
alias gd="git diff"
